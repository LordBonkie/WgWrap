{
  "WireGuard": {
    "ConfigPath": "C:/path/to/your/config.conf",
    "WgExe": "C:/Program Files/WireGuard/wireguard.exe",
    "TrustedSsids": [
      "YourHomeSSID"
    ],
    "TrustedIpRanges": [
      "192.168.1.0/24"
    ],
    "ExcludedNetworkAdapters": [],
    "TimerEnabled": true,
    "TimerIntervalSeconds": 30,
    "AutoStartWithWindows": false,
    "ShowShutdownWarning": true
  }
}
